# RAG Evaluation Environment Variables
# Copy this file to .env and fill in your values

# Local models directory (shared between orchestrator and worker)
LOCAL_MODELS_DIR=/path/to/models

# Worker API URL (for orchestrator to connect to)
WORKER_URL=http://localhost:8000

# Langfuse configuration (optional - for cloud observability)
LANGFUSE_PUBLIC_KEY=pk-your-public-key
LANGFUSE_SECRET_KEY=sk-your-secret-key
LANGFUSE_HOST=https://cloud.langfuse.com

# Logging level
LOG_LEVEL=INFO
